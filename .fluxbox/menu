[begin] (Fluxbox)
  [submenu] (Apps)
    [exec] (XTerm) {~/bin/term}
    [exec] (Vim) {~/bin/term -e /usr/bin/vim}
    [exec] (Browser) {~/bin/browser}
  [end]
  [config] (Config)
  [submenu] (Styles) {}
    [stylesdir] (/usr/share/fluxbox/styles)
    [stylesdir] (~/.fluxbox/styles)
  [end]
  [workspaces] (Workspaces)
  [reconfig] (Reconfigure)
  [restart] (Restart)
  [exit] (Exit)
[end]
